{
  "name": "Vue-Fullstack-Starter",
  "version": "1.0.0",
  "description": "A boilerplate for Vue, Material, Express, Babel, Flow, and ReactiveX.",
  "scripts": {
    "start:app": "cross-env BABEL_ENV=app webpack-dev-server --progress",
    "start:api": "cross-env BABEL_ENV=api gulp",
    "build:app": "cross-env BABEL_ENV=app webpack --env.prod --progress",
    "build:api": "cross-env BABEL_ENV=api gulp --prod",
    "test:app": "cross-env BABEL_ENV=app TEST_ENV=app jest \"src/app/.*\\.js$\" --coverage --forceExit",
    "test:api": "cross-env BABEL_ENV=api TEST_ENV=api jest \"src/api/.*\\.js$\" --coverage --forceExit",
    "e2e:app": "cross-env BABEL_ENV=app jest \"src/app/.*\\.e2e.*\\.(js)$\" --forceExit",
    "e2e:api": "cross-env BABEL_ENV=api jest \"src/api/.*\\.e2e.*\\.(js)$\" --forceExit",
    "typed": "flow-typed install",
    "codecov": "codecov",
    "firebase": "firebase"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Vue-Fullstack-Starter.git"
  },
  "keywords": [
    "Vue",
    "Fullstack"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Vue-Fullstack-Starter/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Vue-Fullstack-Starter#readme",

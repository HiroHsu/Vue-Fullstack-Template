    "lint:css": "stylelint 'src/**/*.{css,vue}'",
    "test:app": "cross-env BABEL_ENV=app JEST_ENV=app jest \"src/app/.*\\.js$\" --coverage --forceExit",
    "test:api": "cross-env BABEL_ENV=api JEST_ENV=api jest \"src/api/.*\\.js$\" --coverage --forceExit",
    "e2e:app": "cross-env BABEL_ENV=app JEST_ENV=e2e jest \"src/app/.*\\.js$\" --forceExit",
    "e2e:api": "cross-env BABEL_ENV=api JEST_ENV=e2e jest \"src/api/.*\\.js$\" --forceExit",
